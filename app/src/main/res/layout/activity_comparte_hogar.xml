<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ComparteHogar">

    <com.google.android.material.chip.Chip
        android:id="@+id/chip4"
        android:layout_width="83dp"
        android:layout_height="46dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toEndOf="@id/chip6"
        app:layout_constraintTop_toBottomOf="@+id/tv_direccion" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip6"
        android:layout_width="83dp"
        android:layout_height="46dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        app:layout_constraintStart_toStartOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/chip4" />

    <TextView
        android:id="@+id/tv_direccion"
        android:layout_width="166dp"
        android:layout_height="26dp"
        android:layout_marginTop="24dp"
        android:gravity="center_horizontal"
        android:hint="Ingrese su direcciÃ³n"
        android:textAlignment="center"
        android:textColor="#04BFA9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text=""
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="@string/TituloVentanaComparteHogar" />

    <com.google.android.material.chip.Chip
        android:id="@+id/chip7"
        android:layout_width="83dp"
        android:layout_height="46dp"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/chip4"
        app:layout_constraintTop_toTopOf="@+id/chip4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.9" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.1" />

    <TableLayout
        android:layout_width="0dp"
        android:layout_height="565dp"

        app:layout_constraintEnd_toEndOf="@id/guideline"
        app:layout_constraintStart_toStartOf="@id/guideline2"
        app:layout_constraintTop_toBottomOf="@+id/chip4"
        tools:layout_editor_absoluteX="16dp">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="164dp"
                android:layout_height="100dp"
                app:srcCompat="@drawable/ic_android_black_24dp"
                tools:ignore="VectorDrawableCompat" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="165dp"
                android:layout_height="100dp"
                app:srcCompat="@drawable/ic_android_black_24dp"
                tools:ignore="DuplicateIds,VectorDrawableCompat" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


    </TableLayout>

</androidx.constraintlayout.widget.ConstraintLayout>